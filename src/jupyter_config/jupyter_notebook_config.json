{
  "NotebookApp": {
    "nbserver_extensions": {
      "jupyter_nbextensions_configurator": true,
      "jupyter_http_over_ws": true,
      "jupyterlab_code_formatter": true
    },
    "extra_template_paths": [
        "/home/jovyan/.jupyter/templates"
    ]
  },

  "Exporter": {
    "preprocessors": [
      "jupyter_contrib_nbextensions.nbconvert_support.CodeFoldingPreprocessor",
      "jupyter_contrib_nbextensions.nbconvert_support.PyMarkdownPreprocessor"
    ],

    "template_path": [
      ".",
      "/home/jovyan/.jupyter/templates",
      "/opt/conda/lib/python3.7/site-packages/jupyter_contrib_nbextensions/templates"
    ]
  },

  "LatexExporter": {
    "template_file": "easy_jupyter"
  },

  "TemplateExporter": {
    "exclude_input_prompt": true,
    "exclude_output_prompt": true
  },

  "NbConvertApp": {
    "postprocessor_class":
      "post_embedhtml.EmbedPostProcessor"
    }
}
